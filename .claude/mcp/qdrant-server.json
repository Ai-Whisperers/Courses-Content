{
  "mcpServers": {
    "qdrant-vector": {
      "command": "npx",
      "args": [
        "-y",
        "@qdrant/mcp-server-qdrant"
      ],
      "env": {
        "QDRANT_URL": "${QDRANT_URL:-http://localhost:6333}",
        "QDRANT_API_KEY": "${QDRANT_API_KEY}",
        "QDRANT_LOCAL_PATH": "${QDRANT_LOCAL_PATH:-./data/qdrant}"
      },
      "description": "Vector search engine for semantic memory and RAG applications",
      "capabilities": ["vector_search", "semantic_memory", "rag", "embeddings"],
      "useCases": [
        "Store and retrieve document embeddings",
        "Semantic search across course materials",
        "Build RAG pipelines for AI assistants",
        "Knowledge management for education"
      ]
    }
  }
}
